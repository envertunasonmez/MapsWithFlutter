<?xml version="1.0" encoding="utf-8"?>
<resources>
    <string name="google_maps_api_key">${GOOGLE_MAPS_API_KEY}</string>

    <!-- Theme applied to the Android Window while the process is starting when Dark Mode is off -->
    <style name="LaunchTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowBackground">@drawable/launch_background</item>
    </style>

    <!-- Theme applied to the Android Window while Flutter UI initializes -->
    <style name="NormalTheme" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowBackground">?android:colorBackground</item>
    </style>
</resources>
